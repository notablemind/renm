{
  "name": "renm",
  "version": "0.1.0",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w -ws _ ",
    "serve": "http-server -p 6521 -c-1",
    "pack": "fpack --development --preprocess='\\.svg$:raw-loader'  src/app/Index.bs.js --no-cache -w --preprocess='\\.css$:style-loader!css-loader'",
    "pack:worker": "fpack --preprocess='\\.svg$:raw-loader' -n SharedWorker.js src/app/SharedWorker.bs.js --no-cache -w --preprocess='\\.css$:style-loader!css-loader'",
    "clean": "bsb -clean-world",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bs-css": "^7.1.0",
    "css-loader": "^1.0.0",
    "fast-deep-equal": "^2.0.1",
    "file-loader": "^2.0.0",
    "fpack": "^0.5.4",
    "html-loader": "^0.5.5",
    "leveljs": "0.0.0-a1",
    "levelup": "^3.1.1",
    "loader-runner": "^2.3.0",
    "quill": "^1.3.6",
    "quill-cursors": "^1.0.3",
    "quill-delta": "^3.6.3",
    "quill-mention": "^2.0.0",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reason-react": ">=0.4.0",
    "style-loader": "^0.23.0",
    "sublevelup": "^4.0.0",
    "url-loader": "^1.1.1"
  },
  "devDependencies": {
    "bs-platform": "^4.0.3"
  }
}
